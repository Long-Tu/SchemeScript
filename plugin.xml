<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.schemeway.plugins.schemescript"
   name="SchemeScript Plug-in"
   version="1.1.3"
   provider-name="The SchemeWay Project"
   class="org.schemeway.plugins.schemescript.SchemeScriptPlugin">

   <runtime>
      <library name="schemescript.jar">
         <export name="*"/>
      </library>
      <library name="lib/kawa.jar">
         <export name="*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.ui.editors"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.ui.ide"/>
      <import plugin="org.eclipse.ui.workbench.texteditor"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.debug.ui"/>
      <import plugin="org.eclipse.ui.console"/>
      <import plugin="org.eclipse.ui.views"/>
      <import plugin="org.eclipse.help"/>
      <import plugin="org.eclipse.jdt"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.jdt.ui"/>
      <import plugin="org.eclipse.jdt.debug"/>
      <import plugin="org.eclipse.jdt.debug.ui"/>
      <import plugin="org.eclipse.pde"/>
      <import plugin="org.eclipse.pde.core"/>
      <import plugin="org.eclipse.pde.ui"/>
      <import plugin="org.eclipse.ui.forms"/>
   </requires>
   <extension-point id="interpreters" name="interpreters" schema="schema/interpreters.exsd"/>
   <extension
         point="org.eclipse.core.runtime.contentTypes">
      <content-type
            base-type="org.eclipse.core.runtime.text"
            id="Scheme"
            name="Scheme File"
            file-extensions="%Extensions"
            priority="normal"/>
      <file-association
            content-type="org.schemeway.plugins.schemescript.Scheme"
            file-extensions="%Extensions"/>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="Scheme Editor"
            default="true"
            icon="icons/scheme.gif"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            class="org.schemeway.plugins.schemescript.editor.SchemeEditor"
            extensions="%Extensions"
            id="org.schemeway.plugins.schemescript.editor.SchemeEditor">
         <contentTypeBinding contentTypeId="org.schemeway.plugins.schemescript.Scheme"/>
      </editor>
   </extension>
   
   <extension
         point="org.eclipse.core.filebuffers.documentSetup">
      <participant
            class="org.schemeway.plugins.schemescript.editor.SchemeDocumentSetupParticipant"
            contentTypeId="org.schemeway.plugins.schemescript.Scheme"
            extensions="%Extensions"/>
   </extension>
   <extension
	   	 point="org.eclipse.core.filebuffers.annotationModelCreation">
      <factory
            class="org.eclipse.ui.texteditor.ResourceMarkerAnnotationModelFactory"
            contentTypeId="org.schemeway.plugins.schemescript.Scheme"
            extensions="%Extensions">
	  </factory>
   </extension>

   <extension
         point="org.eclipse.ui.contexts">
      <context
            name="Editing Scheme"
            description="Scheme Editor Context"
            id="Scheme Editing"
            parentId="org.eclipse.ui.textEditorScope">
      </context>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <activeKeyConfiguration
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </activeKeyConfiguration>
      <category
            name="Scheme Editing"
            description="Scheme-specific editing commands"
            id="Scheme Editing">
      </category>
      <command
            name="Forward S-expression"
            description="Forward S-expression"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.forward">
      </command>
      <command
            name="Select Forward S-expression"
            description="Select Forward S-expression"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.select-forward">
      </command>
      <command
            name="Backward S-expression"
            category="Scheme Editing"
            description="Backward S-expression"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.backward">
      </command>
      <command
            name="Select Backward S-expression"
            category="Scheme Editing"
            description="Select Backward S-expression"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.select-backward">
      </command>
      <command
            name="Up S-expression"
            category="Scheme Editing"
            description="Moves to the start of the enclosing S-expression"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.up">
      </command>
      <command
            name="Select Up S-expression"
            category="Scheme Editing"
            description="Selects the enclosing S-expression"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.select-up">
      </command>
      <command
            name="Restore the previous selection"
            category="Scheme Editing"
            description="Restores the previous selection"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.restore-selection">
      </command>
      <command
            name="Down S-expression"
            category="Scheme Editing"
            description="Enters the next S-expression"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.down">
      </command>
      <command
            name="Swap S-expressions"
            category="Scheme Editing"
            description="Swaps the S-expressions around point"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.swap">
      </command>
      <command
            name="Format Selection"
            category="Scheme Editing"
            description="Formats the selection"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.format">
      </command>
      <command
            name="Expand Selection"
            category="Scheme Editing"
            description="Expands the selection to the enclosing S-expressions"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.expand-selection">
      </command>
      <command
            name="Chapter comment"
            category="Scheme Editing"
            description="Inserts a chapter-style comment"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.comment.chapter">
      </command>
      <command
            name="Section comment"
            category="Scheme Editing"
            description="Inserts a section-style comment"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.comment.section">
      </command>
      <command
            name="Toggle Comment"
            category="Scheme Editing"
            description="Comments/uncomments the selected lines"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.comment.selection">
      </command>
      <command
            name="Scheme file header comment"
            category="Scheme Editing"
            description="Inserts a file header comment"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.comment.header">
      </command>
      <command
            name="Mouse Copy"
            description="Copies the S-expression nears the mouse"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.sexp.mousecopy">
      </command>
      <command
            name="Extended Mouse Copy"
