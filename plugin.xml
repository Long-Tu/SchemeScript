      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.format"
            contextId="Scheme Editing"
            keySequence="TAB"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.expand-selection"
            contextId="Scheme Editing"
