            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.code.expandTypename">
      </command>
      <command
            name="Expand package"
            description="Replace the previous symbol by 'define-namespace' clauses of a whole package"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.code.expandPackage">
      </command>
      <command
            name="Start Interpreter"
            description="Starts the Scheme interpreter and show the console"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.eval.start">
      </command>
      <command
            name="Restart Interpreter"
            description="Restarts the Scheme interpreter and show the console"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.eval.restart">
      </command>
      <command
            name="Interrupt Evaluation"
            description="Interrupts the evaluation and show the console"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.eval.interrupt">
      </command>
      <command
            name="Compress Spaces"
            description="Replaces all surrounding whitespaces by a single space"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.format.compress">
      </command>
      <command
            name="Move Closing Parenthesis"
            description="Move the closing parenthesis past the next S-expression"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.format.moveRparen">
      </command>
      <command
            name="Move Opening Parenthesis"
            description="Move the opening parenthesis in front of the previous S-expression"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.format.moveLparen">
      </command>
      <command
            name="Jump to Definition"
            description="Jump to symbol definition"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.jump.definition">
      </command>
      <command
