            description="Finds a symbol in the dictionary"
            category="Scheme Editing"
            categoryId="Scheme Editing"
            id="org.schemeway.plugins.schemescript.dictionary.find">
      </command>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.forward"
            contextId="Scheme Editing"
            keySequence="ALT+CTRL+F"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.select-forward"
            contextId="Scheme Editing"
            keySequence="ALT+CTRL+SHIFT+F"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.backward"
            contextId="Scheme Editing"
            keySequence="ALT+CTRL+B"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.select-backward"
            contextId="Scheme Editing"
            keySequence="ALT+CTRL+SHIFT+B"
            keyConfigurationId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </keyBinding>
      <keyBinding
            commandId="org.schemeway.plugins.schemescript.sexp.up"
            contextId="Scheme Editing"
